{
  "name": "right-type",
  "version": "0.1.0",
  "description": "A simple utility for checking JavaScript types.",
  "keywords": [
    "simple",
    "utility",
    "right",
    "type",
    "checking",
    "javascript"
  ],
  "homepage": "https://github.com/danieldogeanu/right-type",
  "bugs": {
    "url": "https://github.com/danieldogeanu/right-type/issues",
    "email": "contact@danieldogeanu.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danieldogeanu/right-type.git"
  },
  "author": "Daniel Dogeanu <contact@danieldogeanu.com> (https://danieldogeanu.com/)",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "mocha --watch",
    "test": "mocha",
    "coverage": "nyc mocha",
    "show-coverage": "start ./coverage/index.html",
    "build": "babel src --out-dir lib"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {"targets": {"node": "6.17.1"}}
      ]
    ]
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "include": "src",
    "exclude": "**/*spec.js"
  },
  "mocha": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "spec": "./src/**/*+(spec|test).js"
  }
}
