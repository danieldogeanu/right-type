{
  "name": "right-type",
  "version": "0.2.0",
  "description": "A simple utility for checking JavaScript types.",
  "keywords": [
    "simple",
    "utility",
    "right",
    "type",
    "checking",
    "javascript"
  ],
  "homepage": "https://github.com/danieldogeanu/right-type",
  "bugs": {
    "url": "https://github.com/danieldogeanu/right-type/issues",
    "email": "contact@danieldogeanu.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danieldogeanu/right-type.git"
  },
  "author": "Daniel Dogeanu <contact@danieldogeanu.com> (https://danieldogeanu.com/)",
  "license": "MIT",
  "main": "./lib/index.js",
  "directories": {
    "lib": "./lib"
  },
  "private": false,
  "engines": {
    "node": ">=6.17.1"
  },
  "engineStrict": true,
  "scripts": {
    "start": "mocha --watch",
    "test": "mocha",
    "coverage": "nyc mocha",
    "show-coverage": "http-server coverage -o",
    "build": "babel src --out-dir lib",
    "prepare": "npm test && npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "core-js": "^3.6.5",
    "http-server": "^0.12.3",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "regenerator-runtime": "^0.13.5"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6.17.1"
          }
        }
      ]
    ]
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "include": "src",
    "exclude": "**/*spec.js"
  },
  "mocha": {
    "require": [
      "core-js/stable",
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "spec": "./tests/**/*+(spec|test).js"
  }
}
