{
  "name": "right-type",
  "version": "0.2.3",
  "description": "A simple utility for checking JavaScript types.",
  "keywords": [
    "simple",
    "utility",
    "right",
    "type",
    "checking",
    "javascript"
  ],
  "homepage": "https://github.com/danieldogeanu/right-type",
  "bugs": {
    "url": "https://github.com/danieldogeanu/right-type/issues",
    "email": "contact@danieldogeanu.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danieldogeanu/right-type.git"
  },
  "author": "Daniel Dogeanu <contact@danieldogeanu.com> (https://danieldogeanu.com/)",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "directories": {
    "lib": "./lib"
  },
  "private": false,
  "engines": {
    "node": ">=6.17.1"
  },
  "engineStrict": true,
  "scripts": {
    "start": "mocha --watch",
    "test": "mocha",
    "coverage": "nyc mocha",
    "coverage:show": "http-server coverage -o",
    "delete": "del-cli lib",
    "build": "npm run delete && babel src --out-dir lib",
    "prepare": "npm test && npm run build",
    "update:force": "node scripts/update"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6.17.1"
          }
        }
      ]
    ]
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "include": "src",
    "exclude": "**/*spec.js"
  },
  "mocha": {
    "require": [
      "core-js/stable",
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "spec": "./tests/**/*+(spec|test).js"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/register": "^7.13.8",
    "chai": "^4.3.3",
    "chalk": "^4.1.0",
    "core-js": "^3.9.1",
    "del-cli": "^3.0.1",
    "http-server": "^0.12.3",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "regenerator-runtime": "^0.13.7"
  }
}
